{"input": {
    "kind": "readh5",
    "params":{
        "origin": [],
        "voxelSize": [5,5,45],
        "datasetName": "image",
        "isRemoveSourceFile": false
    },
    "inputs": {
        "fileName": "../assets/zfish_25397_8001_1.img.h5"
    },
    "outputs": {
        "data": "img"
    }
},
"saveImg":{
    "kind": "savechunk",
    "params":{},
    "inputs":{
        "chunk": "img"
    },
    "outputs":{
        "chunkFileName": "/tmp/img.h5"
    }
},
"MSF":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "caffeModelFile": "/opt/kaffe/models/MSF/deploy.prototxt",
        "caffeNetFile": "/opt/kaffe/experiments/zfish/MSF/train_iter_250000.caffemodel.h5",
        "outputPatchSize":    [150, 150,  12],
        "preprocess": "2dnormalize",
        "scanParams": "None",
        "deviceID": 0,
        "isCropImg": true,
        "cropMarginSize": [6,   6,    0],
        "originOffset": [54, 54, 4],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"JNet":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "caffeModelFile": "/opt/kaffe/models/JNet/deploy.prototxt",
        "caffeNetFile": "/opt/kaffe/experiments/zfish/JNet/train_iter_215000.caffemodel.h5",
        "outputPatchSize":    [158, 158,  32],
        "preprocess": "2dnormalize",
        "scanParams": "dict(stride=(0.5,0.5,0.5), blend='bump')",
        "deviceID": 0,
        "isCropImg": false,
        "cropMarginSize": [54,  54,   4],
        "originOffset": [54, 54, 4],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"cropImg":{
  "kind": "crop",
  "params": {
    "cropMarginSize": [54, 54, 4]
  },
  "inputs":{
    "img": "img"
  },
  "outputs":{
    "img": "img"
  }
},
"atomicseg":{
    "kind": "atomicseg",
    "params": {
        "low": 0.1,
        "high": 0.8,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.2
            }
        ],
        "dust": 600,
        "isThresholdRelative": true
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "chunkFileName": "/tmp/sgm.h5"
    }
}
}
