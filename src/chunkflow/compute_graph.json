{"input": {
    "kind": "readh5",
    "params": {
        "fname": "s3://spipe-service/assets/piriform.209x209x10.img.h5",
        "dname": "main"
    },
    "outputs": ["img"]
},
"znni":{
    "kind": "inference",
    "params":{
        "outsz": [64, 64, 3]
    },
    "inputs": ["img"],
    "outputs": ["aff"]
},
"crop_img":{
    "kind": "crop",
    "params": {
        "cropsize": [54, 54, 4]
    },
    "inputs": ["img"],
    "outputs": ["img"]
},
"crop_aff":{
    "kind": "crop",
    "params": {
        "cropsize": [54, 54, 4]
    },
    "inputs": ["aff"],
    "outputs": ["aff"]
},
"watershed":{
    "kind": "watershed",
    "params": {
        "low": 0.1,
        "high": 0.7,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.2
            }
        ],
        "dust": 800
    },
    "inputs": ["aff"],
    "outputs": ["sgm"]
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean"
    },
    "inputs": ["sgm", "aff"],
    "outputs": ["sgm"]
},
"omnification":{
    "kind": "omnification",
    "params":{
        "voxelsize": [5,5,45],
        "ombin": "/opt/omni",
        "fout": "/tmp/"
    },
    "inputs": ["img", "sgm"],
    "outputs": []
}
}
