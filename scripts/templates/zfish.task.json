{"input": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "aligned",
        "origin":   [32955, 10187, 1],
        "chunkSize": [1132, 1132,  136],
        "voxelSize": [5,5,45],
        "nonzeroRatioThreshold": 0.75
    },
    "inputs": {
      "registerFile": "/mnt/data01/datasets/zebrafish/4_aligned/registry.txt"
    },
    "outputs": {
        "data": "img"
    }
},
"saveimg":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "img"
    },
    "outputs": {
        "prefix": "gs://jpwu/zfish/old3nets/image/chunk_"
    }
},
"znni":{
    "kind": "znni",
    "params":{
        "znniBinaryFile": "/opt/ZNNi/code/bin/multiscale/znni_gpu",
        "deviceID": 0,
        "fnet": "s3://znn/experiments/zfish/net_290000.h5",
        "outputPatchSize": [256, 256, 17],
        "fieldOfView": [109, 109, 9],
        "isExchangeAffXZ": true,
        "affWeight": 0.333333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"MSF":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "caffeModelFile": "/opt/kaffe/caffe.models/MSF/deploy.prototxt",
        "caffeNetFile": "/opt/kaffe/experiments/zfish/MSF/train_iter_250000.caffemodel.h5",
        "outputPatchSize":    [150, 150,  12],
        "scanParams": "None",
        "preprocess": "normalize",
        "deviceID": 0,
        "isCropImg": true,
        "cropMarginSize": [6,   6,    0],
        "originOffset": [54, 54, 4],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"JNet":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "caffeModelFile": "gs://jpwu/kaffe/caffe.models/JNet/deploy.prototxt",
        "caffeNetFile": "gs://jpwu/kaffe/experiments/zfish/JNet/train_iter_325000.caffemodel.h5",
        "outputPatchSize":    [158, 158,  32],
        "preprocess": "normalize",
        "scanParams": "dict(stride=(0.5,0.5,0.5), blend='bump')",
        "deviceID": 0,
        "isCropImg": false,
        "cropMarginSize": [54,  54,   4],
        "originOffset": [54, 54, 4],
        "affWeight": 0.333333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"saveaff":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "aff"
    },
    "outputs": {
        "prefix": "gs://jpwu/zfish/old3nets/affinityMap/chunk_"
    }
},
"cropImg":{
  "kind": "crop",
  "params": {
    "cropMarginSize": [54, 54, 4]
  },
  "inputs":{
    "img": "img"
  },
  "outputs":{
    "img": "img"
  }
},
"watershed":{
    "kind": "atomicseg",
    "params": {
        "low": 0.003655,
        "high": 0.999992,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.021569
            }
        ],
        "dust": 600,
        "isThresholdRelative": false
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "gs://jpwu/zfish/old3nets/segmentation/chunk_"
    }
},
"hypersquare":{
    "kind": "hypersquare",
    "params":{
        "segment_id_type" : "UInt16",
        "affinity_type" : "Float32"
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "projectsDirectory": "gs://jpwu/zfish/old3nets/hypersquare/"
    }
},
"omnification":{
    "kind": "omnification",
    "params":{
        "ombin": "/opt/omni",
        "isMeshing": false
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "prefix": "/tmp/chunk_"
    }
},
"moveomniprj":{
    "kind": "movedata",
    "params": {
      "isRemoveSourceFile": true
    },
    "inputs": {
        "prefix": "/tmp/chunk_"
    },
    "outputs": {
        "dir": "~/seungmount/research/Jingpeng/14_zfish/old3nets/"
    }
}
}
