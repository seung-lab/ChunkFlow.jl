{"input": {
    "kind": "readh5",
    "params":{
        "origin": [],
        "voxelsize": [5,5,45],
        "datasetName": "image"
    },
    "inputs": {
        "fname": "../assets/zfish_25397_8001_1.img.h5"
    },
    "outputs": {
        "data": "img"
    }
},
"znni":{
    "kind": "znni",
    "params":{
        "fznni": "/opt/ZNNi/code/bin/multiscale/znni_gpu",
        "GPUID": 0,
        "fnet": "~/seungmount/research/kisuklee/Sharing/Jingpeng/blend_test/MS/net_290000.h5",
        "outsz": [256, 256, 17],
        "fov": [109, 109, 9],
        "isexchangeaffxz": true,
        "affWeight": 0.333333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"saveImg":{
    "kind": "savechk",
    "params":{
        "isdelete": false
    },
    "inputs":{
        "chunk": "img"
    },
    "outputs":{
        "fname": "/tmp/img.h5"
    }
},
"MSF":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "fModel": "/opt/kaffe/models/MSF/deploy.prototxt",
        "fNet": "/opt/kaffe/experiments/zfish/MSF/train_iter_250000.caffemodel.h5",
        "outsz":    [150, 150,  12],
        "scanParams": "None",
        "GPUID": 0,
        "isCropImg": true,
        "cropMarginSize": [6,   6,    0],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"JNet":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "fModel": "/opt/kaffe/models/JNet/deploy.prototxt",
        "fNet": "/opt/kaffe/experiments/zfish/JNet/train_iter_215000.caffemodel.h5",
        "outsz":    [158, 158,  32],
        "scanParams": "dict(stride=(0.5,0.5,0.5), blend='bump')",
        "GPUID": 0,
        "isCropImg": false,
        "cropMarginSize": [54,  54,   4],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"cropImg":{
  "kind": "crop",
  "params": {
    "cropMarginSize": [54, 54, 4]
  },
  "inputs":{
    "img": "img"
  },
  "outputs":{
    "img": "img"
  }
},
"atomicseg":{
    "kind": "atomicseg",
    "params": {
        "low": 0.1,
        "high": 0.8,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.2
            }
        ],
        "dust": 600,
        "isThresholdRelative": true
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isdeleteaff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechk",
    "params": {
        "isdelete": true
    },
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "fname": "/tmp/sgm.h5"
    }
},
"omnification":{
    "kind": "omnification",
    "params":{
        "ombin": "/opt/omni",
        "isMeshing": true
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "fprj": "/tmp/chunk_"
    }
}
}
