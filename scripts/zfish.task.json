{"input": {
    "kind": "readh5",
    "params":{
        "origin": [],
        "voxelsize": [5,5,45],
        "datasetName": "main",
        "isRemoveSourceFile": true
    },
    "inputs": {
        "fileName": "s3://zfish/dodam_aligned_stacks/2x2x2_2/zebrafish_"
    },
    "outputs": {
        "data": "img"
    }
},
"znni":{
    "kind": "znni",
    "params":{
        "znniBinaryFile": "/opt/ZNNi/code/bin/multiscale/znni_gpu",
        "GPUID": 0,
        "fnet": "s3://znn/experiments/zfish/net_290000.h5",
        "outputPatchSize": [256, 256, 17],
        "fieldOfView": [109, 109, 9],
        "isExchangeAffXZ": true,
        "affWeight": 0.333333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"MSF":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "caffeModelFile": "/opt/kaffe/models/MSF/deploy.prototxt",
        "caffeNetFile": "/opt/kaffe/experiments/zfish/MSF/train_iter_250000.caffemodel.h5",
        "outputPatchSize":    [150, 150,  12],
        "scanParams": "None",
        "GPUID": 0,
        "isCropImg": true,
        "cropMarginSize": [6,   6,    0],
        "originOffset": [54, 54, 4],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"JNet":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "caffeModelFile": "/opt/kaffe/models/JNet/deploy.prototxt",
        "caffeNetFile": "/opt/kaffe/experiments/zfish/JNet-f/train_iter_325000.caffemodel.h5",
        "outputPatchSize":    [158, 158,  32],
        "scanParams": "dict(stride=(0.5,0.5,0.5), blend='bump')",
        "GPUID": 0,
        "isCropImg": false,
        "cropMarginSize": [54,  54,   4],
        "originOffset": [54, 54, 4],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"saveaff":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "aff"
    },
    "outputs": {
        "prefix": "s3://zfish/dodam_aligned_stacks/2x2x2_2/affinityMap/chunk_"
    }
},
"cropImg":{
  "kind": "crop",
  "params": {
    "cropMarginSize": [54, 54, 4]
  },
  "inputs":{
    "img": "img"
  },
  "outputs":{
    "img": "img"
  }
},
"watershed":{
    "kind": "atomicseg",
    "params": {
        "low": 0.1,
        "high": 0.8,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.2
            }
        ],
        "dust": 600,
        "isThresholdRelative": true
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "s3://zfish/dodam_aligned_stacks/2x2x2_2/segmentation/chunk_"
    }
},
"hypersquare":{
    "kind": "hypersquare",
    "params":{
        "segment_id_type" : "UInt16",
        "affinity_type" : "Float32"
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "projectsDirectory": "s3://zfish/dodam_aligned_stacks/2x2x2_2/hypersquare/"
    }
}
}
