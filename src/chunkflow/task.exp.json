{"input": {
    "kind": "readh5",
    "params": {
        "fname": "s3://spipe-service/assets/piriform.209x209x10.img.h5",
        "dname": "main",
        "voxelsize": [5,5,45]
    },
    "outputs": ["img"]
},
"znni":{
    "kind": "znni",
    "params":{
        "fznni": "/opt/ZNNi/code/multiscale/znni",
        "outsz": [8, 8, 1],
        "fov": [109, 109, 9]
    },
    "inputs": ["img"],
    "outputs": ["img", "aff"]
},
"watershed":{
    "kind": "watershed",
    "params": {
        "low": 0.1,
        "high": 0.8,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.2
            }
        ],
        "dust": 600
    },
    "inputs": ["aff"],
    "outputs": ["sgm"]
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean"
    },
    "inputs": ["sgm", "aff"],
    "outputs": ["sgm"]
},
"omnification":{
    "kind": "omnification",
    "params":{
        "ombin": "/opt/omni",
        "fprj": "/tmp/"
    },
    "inputs": ["img", "sgm"],
    "outputs": []
}
}
