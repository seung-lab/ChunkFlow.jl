{"input": {
    "kind": "readh5",
    "params":{
        "origin": [],
        "voxelsize": [5,5,45],
        "datasetName": "img",
        "isRemoveSourceFile": true
    },
    "inputs": {
        "fname": "s3://zfish/dodam_aligned_stacks/8x8x8/zebrafish_aligned_"
    },
    "outputs": {
        "data": "img"
    }
},
"znni":{
    "kind": "znni",
    "params":{
        "fznni": "/opt/ZNNi/code/bin/multiscale/znni_gpu",
        "GPUID": 0,
        "fnet": "s3://znn/experiments/zfish/net_290000.h5",
        "outsz": [256, 256, 17],
        "fov": [109, 109, 9],
        "isexchangeaffxz": true,
        "affWeight": 0.333333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"MSF":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "fModel": "/opt/kaffe/models/MSF/deploy.prototxt",
        "fNet": "/opt/kaffe/experiments/zfish/MSF/train_iter_250000.caffemodel.h5",
        "outsz":    [150, 150,  12],
        "scanParams": "None",
        "GPUID": 0,
        "isCropImg": true,
        "cropMarginSize": [6,   6,    0],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"JNet":{
    "kind": "kaffe",
    "params":{
        "kaffeDir": "/opt/kaffe",
        "fModel": "/opt/kaffe/models/JNet/deploy.prototxt",
        "fNet": "/opt/kaffe/experiments/zfish/JNet-f/train_iter_325000.caffemodel.h5",
        "outsz":    [158, 158,  32],
        "scanParams": "dict(stride=(0.5,0.5,0.5), blend='bump')",
        "GPUID": 0,
        "isCropImg": false,
        "cropMarginSize": [54,  54,   4],
        "affWeight": 0.33333333
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"saveaff":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "aff"
    },
    "outputs": {
        "prefix": "s3://zfish/dodam_aligned_stacks/8x8x8/affinityMap/chunk_"
    }
},
"cropImg":{
  "kind": "crop",
  "params": {
    "cropMarginSize": [54, 54, 4]
  },
  "inputs":{
    "img": "img"
  },
  "outputs":{
    "img": "img"
  }
},
"watershed":{
    "kind": "atomicseg",
    "params": {
        "low": 0.1,
        "high": 0.8,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.2
            }
        ],
        "dust": 600,
        "isThresholdRelative": true
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "s3://zfish/dodam_aligned_stacks/8x8x8/segmentation/chunk_"
    }
},
"omnification":{
    "kind": "omnification",
    "params":{
        "ombin": "/opt/omni",
        "isMeshing": false
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "fprj": "/tmp/chunk_"
    }
},
"moveomniprj":{
    "kind": "movedata",
    "params": {
      "isRemoveSourceFile": true
    },
    "inputs": {
        "prefix": "/tmp/chunk_"
    },
    "outputs": {
        "dir": "s3://zfish/dodam_aligned_stacks/8x8x8/omniProjects/"
    }
}
}
