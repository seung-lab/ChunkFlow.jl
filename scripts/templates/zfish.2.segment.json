{"input": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "h5s",
        "origin":       [19457, 9217,  129, 1],
        "chunkSize":    [1152,  1152,   144, 3],
        "voxelSize":    [5,     5,      45],
        "nonzeroRatioThreshold": 0.01
    },
    "inputs": {
      "h5sDir": "~/seungmount/research/Jingpeng/14_zfish/jknet/4x4x4/affinitymap/"
    },
    "outputs": {
        "data": "aff"
    }
},
"saveaff":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "aff"
    },
    "outputs": {
        "prefix": "gs://zfish/jknet_5/affinitychunks/chunk_"
    }
},
"input_img": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "aligned",
        "voxelSize":    [5,     5,      45]
    },
    "inputs": {
      "registerFile": "/mnt/data01/datasets/zebrafish/4_aligned/registry.txt",
      "referenceChunk": "aff"
    },
    "outputs": {
        "data": "img"
    }
},
"maskaff":{
    "kind": "maskaffinity",
    "params": {
        "threshold": 1
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"cropimg":{
    "kind": "crop",
    "params": {
        "cropMarginSize": [64, 64, 8]
    },
    "inputs":{
        "img": "img"
    },
    "outputs":{
        "img": "img"
    }
},
"watershed":{
    "kind": "atomicseg",
    "params": {
        "low": 0.003655,
        "high": 0.999999,
        "thresholds": [
            {
                "size": 600,
                "threshold": 0.021569
            }
        ],
        "dust": 600,
        "isThresholdRelative": false,
        "cropSegMarginSize": [64,64,8]
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"cropaff":{
    "kind": "crop",
    "params": {
        "cropMarginSize": [64, 64, 8, 0]
    },
    "inputs":{
        "img": "aff"
    },
    "outputs":{
        "img": "aff"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "gs://zfish/jknet_5/segmentation/chunk_"
    }
},
"hypersquare":{
    "kind": "hypersquare",
    "params":{
        "segment_id_type" : "UInt16",
        "affinity_type" : "Float32"
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "projectsDirectory": "gs://zfish/jknet_5/hypersquare/"
    }
},
"omnification":{
    "kind": "omnification",
    "params":{
        "ombin": "/opt/omni",
        "isMeshing": false
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "prefix": "~/seungmount/research/Jingpeng/14_zfish/jknet/4x4x4/omni/chunk_"
    }
}
}
