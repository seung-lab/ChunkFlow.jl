{"input": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "s3",
        "origin":   [50177, 50177, 16385],
        "chunkSize": [1152, 1152,  144],
        "voxelSize": [4,4,40],
        "nonzeroRatioThreshold": 0.01
    },
    "inputs": {
      "path": "s3://seunglab/pinky40/images/"
    },
    "outputs": {
        "data": "img"
    }
},
"input_aff": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "s3",
        "origin":   [53249, 53249, 16641, 1],
        "chunkSize": [1152, 1152,  144, 3],
        "voxelSize": [4,4,40],
        "nonzeroRatioThreshold": 0.01
    },
    "inputs": {
      "path": "s3://seunglab/pinky40/affinitymap/"
    },
    "outputs": {
        "data": "aff"
    }
},
"cropimg":{
    "kind": "crop",
    "params": {
        "cropMarginSize": [64, 64, 8]
    },
    "inputs":{
        "img": "img"
    },
    "outputs":{
        "img": "img"
    }
},
"watershed":{
    "kind": "atomicseg",
    "params": {
        "low": 0.012,
        "high": 0.999806,
        "thresholds": [
            {
                "size": 600,
                "threshold": 0.087818
            }
        ],
        "dust": 300,
        "isThresholdRelative": false
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "cropSegMarginSize": [64,64,8],
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "s3://seunglab/pinky40/segmentation/chunk_"
    }
},
"hypersquare":{
    "kind": "hypersquare",
    "params":{
        "segment_id_type" : "UInt16",
        "affinity_type" : "Float32"
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "projectsDirectory": "gs://seunglab/pinky40/hypersquare/"
    }
}
}
