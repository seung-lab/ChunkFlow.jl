{"input": {
    "kind": "readh5",
    "params":{
        "origin": [],
        "voxelsize": [4,4,40],
        "datasetName": "img",
        "isRemoveSourceFile": true
    },
    "inputs": {
        "fname": "s3://seunglab/datasets/pinky/export/2048x2048x256/pinky_aligned_"
    },
    "outputs": {
        "data": "img"
    }
},
"znni":{
    "kind": "znni",
    "params":{
        "fznni": "/opt/ZNNi/code/bin/multiscale/znni_gpu",
        "GPUID": 0,
        "fnet": "s3://znn/experiments/allen/0722_VD2D3D-MS/net_current.h5",
        "outsz": [256, 256, 17],
        "fov": [109, 109, 9],
        "isexchangeaffxz": false,
        "affWeight": 1
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"saveaff":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "aff"
    },
    "outputs": {
        "prefix": "~/seungmount/research/Jingpeng/13_neuromancer/pinky/chunk_"
    }
},
"cropImg":{
  "kind": "crop",
  "params": {
    "cropMarginSize": [54, 54, 4]
  },
  "inputs":{
    "img": "img"
  },
  "outputs":{
    "img": "img"
  }
},
"saveimg":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "img"
    },
    "outputs": {
        "prefix": "~/seungmount/research/Jingpeng/13_neuromancer/pinky/chunk_"
    }
},
"watershed":{
    "kind": "atomicseg",
    "params": {
        "low": 0.1,
        "high": 0.8,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.2
            }
        ],
        "dust": 600,
        "isThresholdRelative": true
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "~/seungmount/research/Jingpeng/13_neuromancer/pinky/chunk_"
    }
},
"omnification":{
    "kind": "omnification",
    "params":{
        "ombin": "/opt/omni",
        "isMeshing": true
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "fprj": "/tmp/chunk_"
    }
},
"moveomniprj":{
    "kind": "movedata",
    "params": {
      "isRemoveSourceFile": true
    },
    "inputs": {
        "prefix": "/tmp/chunk_"
    },
    "outputs": {
        "dir": "~/seungmount/research/Jingpeng/13_neuromancer/pinky/"
    }
}
}
