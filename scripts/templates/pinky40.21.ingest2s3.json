{"input": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "aligned",
        "origin":   [1, 1, 1],
        "chunkSize": [2048, 2048,  64],
        "offset":    [16384,16384,0],
        "voxelSize": [4,4,40],
        "nonzeroRatioThreshold": 0.01
    },
    "inputs": {
      "registerFile": "/mnt/data01/datasets/pinky40-3/5_finished/registry.txt"
    },
    "outputs": {
        "data": "img"
    }
},
"blend2cloud":{
    "kind": "blendchunk",
    "params":{
      "backend": "s3"
    },
    "inputs": {
        "chunk": "img"
    },
    "outputs": {
        "path": "s3://seunglab/pinky40_2/image/4_4_40"
    }
}
}
