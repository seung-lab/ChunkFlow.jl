        "cutoutSize": [2112, 2112,  136],
        "voxelSize": [4,4,40],
        "nonzeroRatioThreshold": 0.01
    },
    "inputs": {
      "path": "s3://neuroglancer/pinky40_v10/image/4_4_40/"
    },
    "outputs": {
        "data": "img"
    }
},
"CPUInferenceUNet":{
    "kind": "NodeInference",
    "params":{
