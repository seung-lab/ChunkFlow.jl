{"input": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "h5s",
        "origin":       [10241, 16385,  4003],
        "chunkSize":    [2304,  2304,   256],
        "voxelSize":    [4,     4,      40]
    },
    "inputs": {
      "h5sDir": "/mnt/data02/jingpeng/pinky/affinitymap/"
    },
    "outputs": {
        "data": "aff"
    }
},
"saveaff":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "aff"
    },
    "outputs": {
        "prefix": "s3://seunglab/pinky/all/affinitychunks/chunk_"
    }
},
"atomicseg":{
    "kind": "atomicseg",
    "params": {
        "low": 0.022009,
        "high": 0.999998,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.206746
            }
        ],
        "dust": 600,
        "isThresholdRelative": false,
        "cropSegMarginSize": [128,128,0]
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"cropaff":{
  "kind": "crop",
  "params": {
    "cropMarginSize": [128, 128, 0]
  },
  "inputs":{
    "img": "aff"
  },
  "outputs":{
    "img": "aff"
  }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "s3://seunglab/pinky/all/segmentation/chunk_"
    }
},
"input_img": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "aligned",
        "voxelSize":    [4,     4,      40]
    },
    "inputs": {
      "registerFile": "/mnt/data01/datasets/pinky/4_aligned/registry.txt",
      "referenceChunk": "sgm"
    },
    "outputs": {
        "data": "img"
    }
},
"saveimg":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "img"
    },
    "outputs": {
        "prefix": "s3://seunglab/pinky/all/imagechunks/chunk_"
    }
},
"omnification":{
    "kind": "omnification",
    "params":{
        "ombin": "/opt/omni",
        "isMeshing": true
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "prefix": "/mnt/data02/jingpeng/pinky/omniprojects/chunk_"
    }
}
}
