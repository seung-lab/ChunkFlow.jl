{"input": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "h5s",
        "origin":       [19457,9217,129,1],
        "offset":       [16384,16384,16384,0],
        "chunkSize":    [1152,  1152,   144, 3],
        "voxelSize":    [5,     5,      45],
        "nonzeroRatioThreshold": 0.01,
        "isRemoveNaN": true
    },
    "inputs": {
      "h5sDir": "/mnt/data02/jingpeng/zfish/affinitymap/"
    },
    "outputs": {
        "data": "aff"
    }
},
"saveaff":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "aff"
    },
    "outputs": {
        "prefix": "gs://zfish/all/affinitychunks/chunk_"
    }
},
"input_img": {
    "kind": "cutoutchunk",
    "params":{
        "bigArrayType": "aligned",
        "voxelSize":    [5,     5,      45],
        "origin":       [19457,9217,129],
        "offset":       [16384,16384,16384]
    },
    "inputs": {
      "registerFile": "/mnt/data01/datasets/zebrafish/4_aligned/registry.txt"
    },
    "outputs": {
        "data": "img"
    }
},
"maskaff":{
    "kind": "maskaffinity",
    "params": {
        "threshold": 1
    },
    "inputs": {
        "img": "img",
        "aff": "aff"
    },
    "outputs": {
        "aff": "aff"
    }
},
"cropimg":{
    "kind": "crop",
    "params": {
        "cropMarginSize": [64, 64, 8]
    },
    "inputs":{
        "img": "img"
    },
    "outputs":{
        "img": "img"
    }
},
"watershed":{
    "kind": "atomicseg",
    "params": {
        "low": 0.006821,
        "high": 0.999996,
        "thresholds": [
            {
                "size": 800,
                "threshold": 0.04515
            }
        ],
        "dust": 600,
        "isThresholdRelative": false,
        "cropSegMarginSize": [64,64,8]
    },
    "inputs": {
        "aff": "aff"
    },
    "outputs": {
        "seg": "seg"
    }
},
"cropaff":{
    "kind": "crop",
    "params": {
        "cropMarginSize": [64, 64, 8, 0]
    },
    "inputs":{
        "img": "aff"
    },
    "outputs":{
        "img": "aff"
    }
},
"agglomeration":{
    "kind":"agglomeration",
    "params":{
        "mode": "mean",
        "maskAffinityMarginSize": [32,32,4,0],
        "isDeleteAff": true
    },
    "inputs": {
        "seg": "seg",
        "aff": "aff"
    },
    "outputs": {
        "sgm": "sgm"
    }
},
"savesgm":{
    "kind": "savechunk",
    "params": {},
    "inputs": {
        "chunk": "sgm"
    },
    "outputs": {
        "prefix": "gs://zfish/all/segmentation/chunk_"
    }
},
"hypersquare":{
    "kind": "hypersquare",
    "params":{
        "segment_id_type" : "UInt16",
        "affinity_type" : "Float32"
    },
    "inputs": {
        "img": "img",
        "sgm": "sgm"
    },
    "outputs": {
        "projectsDirectory": "gs://zfish/all/hypersquare/"
    }
}
}
